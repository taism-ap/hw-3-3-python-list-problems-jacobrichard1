
#create a function
def find_maximum(l):
  #set x equal to one less then the lowest possible integer in the list
  x = -1 
  #make the for loop as i in list l
  for i in l:
    #if any element is greater then x, x becomes the value of that element
    if i > x:
      x = i
    
  return x

print ("the largest value is:")
print (find_maximum(l))

